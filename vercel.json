{
  "version": 2,
  "functions": {
    "api/og.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/og",
      "destination": "/api/og"
    },
    {
      "source": "/ogimage/:path*",
      "destination": "/ogimage/:path*"
    },
    {
      "source": "/((?!robots\\.txt|sitemap\\.xml).*)",
      "destination": "/index.html"
    }
  ]
}